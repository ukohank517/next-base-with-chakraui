		"@types/node": "^24.3.0",
		"@types/react": "^19.1.12",
		"firebase": "^12.1.0",
		"framer-motion": "^12.23.12",
		"next": "^15.5.2",
		"next-themes": "^0.4.6",
		"prisma": "^6.15.0",
		"react": "^19.1.1",
