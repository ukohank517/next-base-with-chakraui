generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("PRISMA_DATABASE_URL")
}

model staff {
  staff_id   Int      @id @default(autoincrement())
  staff_names String   @unique
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now()) @updatedAt
}
